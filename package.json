{
  "name": "moslm-roadmap",
  "version": "1.0.0",
  "description": "MERN Roadmap Application",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "npm start --prefix client",
    "dev": "nodemon server.js",
    "data:import": "node backend/seeder",
